{
  "name": "geocode-api",
  "version": "1.0.0",
  "description": "API for querying administrative boundary data from parquet files",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "download": "mkdir -p data && curl -L -o data/whosonfirst-data-admin.parquet https://data.geocode.earth/wof/dist/parquet/whosonfirst-data-admin-latest.parquet"
  },
  "keywords": [
    "geocode",
    "reverse-geocode",
    "duckdb",
    "whosonfirst"
  ],
  "author": "Russell Porter",
  "license": "MIT",
  "dependencies": {
    "@duckdb/node-api": "1.4.0-r.1",
    "express": "5.1.0"
  },
  "devDependencies": {
    "@types/express": "5.0.0",
    "@types/node": "22.10.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3"
  }
}
